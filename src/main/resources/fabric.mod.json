{
  "schemaVersion": 1,
  "id": "mru",
  "version": "${version}",
  "name": "Mineblock's Repetition Utilities",
  "provides": [
    "minelib",
    "mine11lib",
    "hexalib"
  ],
  "description": "Library mod required for most mods by mineblock11",
  "authors": [
    "mineblock11"
  ],
  "contact": {
    "repo": "https://github.com/mineblock11/MRU"
  },
  "license": "MIT",
  "icon": "minimal_64h.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "mine.block.mru.client.MRUClient"
    ],
    "main": [
      "mine.block.mru.MRU"
    ],
    "preLaunch": [
      "mine.block.mru.MRUPrelaunch"
    ]
  },
  "mixins": [
    "mru.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14.9",
    "fabric": "*",
    "minecraft": ">=1.19.2"
  }
}
